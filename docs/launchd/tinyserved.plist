<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>dev.tinyserve.daemon</string>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/local/bin/tinyserved</string>
  </array>
  <key>WorkingDirectory</key>
  <string>$HOME/Library/Application Support/tinyserve</string>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <true/>
  <key>StandardOutPath</key>
  <string>$HOME/Library/Application Support/tinyserve/logs/tinyserved.log</string>
  <key>StandardErrorPath</key>
  <string>$HOME/Library/Application Support/tinyserve/logs/tinyserved.err</string>
</dict>
</plist>
